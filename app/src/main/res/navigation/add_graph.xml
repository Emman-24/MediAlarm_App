<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/add_graph"
    app:startDestination="@id/addMedicineFragment">

    <fragment
        android:id="@+id/addMedicineFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.AddMedicineFragment"
        android:label="AddMedicineFragment"
        tools:layout="@layout/fragment_add_medicine">
        <action
            android:id="@+id/addMedicineFragment_to_addScheduleFragment"
            app:destination="@id/addScheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/addScheduleFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.AddScheduleFragment"
        android:label="fragment_add_schedule"
        tools:layout="@layout/fragment_add_schedule" >
        <action
            android:id="@+id/addScheduleFragment_to_addCyclicFragment"
            app:destination="@id/addCyclicFragment" />
        <action
            android:id="@+id/addScheduleFragment_to_addTimesMultipleFragment"
            app:destination="@id/addTimesMultipleFragment" />
        <action
            android:id="@+id/addScheduleFragment_to_addTimesIntervalHoursFragment"
            app:destination="@id/addTimesIntervalHoursFragment" />
        <action
            android:id="@+id/addScheduleFragment_to_addTimesSpecificFragment"
            app:destination="@id/addTimesSpecificFragment" />
        <action
            android:id="@+id/addScheduleFragment_to_addTimesIntevalDaysFragment"
            app:destination="@id/addTimesIntervalDaysFragment" />
    </fragment>
    <fragment
        android:id="@+id/addCyclicFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.cyclic.AddCyclicFragment"
        android:label="fragment_add_times_cyclic"
        tools:layout="@layout/fragment_add_cyclic" >
        <action
            android:id="@+id/action_addCyclicFragment_to_addTimesCyclicFragment"
            app:destination="@id/addTimesCyclicFragment" />
    </fragment>
    <fragment
        android:id="@+id/addTimesMultipleFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.AddTimesMultipleFragment"
        android:label="fragment_add_times_multiple"
        tools:layout="@layout/fragment_add_times_multiple" />
    <fragment
        android:id="@+id/addTimesSpecificFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.specific.AddTimesSpecificFragment"
        android:label="fragment_add_times_specific"
        tools:layout="@layout/fragment_add_times_specific" />
    <fragment
        android:id="@+id/addTimesIntervalHoursFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.interval.AddTimesIntervalHoursFragment"
        android:label="fragment_add_times_interval"
        tools:layout="@layout/fragment_add_times_interval_hours" />
    <fragment
        android:id="@+id/addTimesCyclicFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.cyclic.AddTimesCyclicFragment"
        android:label="fragment_add_times_cyclic"
        tools:layout="@layout/fragment_add_times_cyclic" />
    <fragment
        android:id="@+id/addTimesIntervalDaysFragment"
        android:name="com.emman.android.medialarm.presentation.ui.addMedicine.interval.AddTimesIntervalDaysFragment"
        android:label="fragment_add_times_inteval_days"
        tools:layout="@layout/fragment_add_times_inteval_days" />
</navigation>